<!DOCTYPE web-app PUBLIC '-//Sun Microsystems, Inc.//DTD
  Web Application 2.3//EN' 'http://java.sun.com/dtd/web-app_2_3.dtd'>
   
<web-app>
	<display-name>Welcomme</display-name>
	<description>
		Welcomme to hello world
	</description>

	<servlet>
		<servlet-name>hello</servlet-name>
		<servlet-class>students.web.HelloWorldServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>hello</servlet-name>
		<url-pattern>/hello</url-pattern>
	</servlet-mapping>

	<taglib>
		<taglib-uri>
			http://127.0.0.1:8080/studentsApp
		</taglib-uri>
		<taglib-location>
			/WEB-INF/hello.tld
		</taglib-location>
	</taglib>
</web-app>
